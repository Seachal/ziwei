        <item name="roundingBorderColor">@color/color19000000</item>
        <item name="roundingBorderWidth">1px</item>
        <item name="placeholderImage">@drawable/blank_icon_avatar</item>
        <item name="roundAsCircle">true</item>
    </style>

    <style name="copy_btn">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12dp</item>
        <item name="android:background">@drawable/copy_btn_selector</item>
        <item name="android:paddingTop">@dimen/space_2</item>
        <item name="android:paddingBottom">@dimen/space_2</item>
        <item name="android:paddingLeft">@dimen/space_5</item>
        <item name="android:paddingRight">@dimen/space_5</item>
    </style>

    <style name="my_edit_text">
        <item name="android:textColor">@color/color333333</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColorHint">@color/colorAAAAAA</item>
        <item name="android:paddingLeft">14dp</item>
        <item name="android:paddingRight">14dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="loading" parent="android:Widget.ProgressBar.Large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="my_check_item" parent="my_item_style">
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color2E2E2E</item>
        <item name="android:textSize">16dp</item>
        <item name="android:checkMark">@drawable/title_icon_opt_selector</item>
    </style>

    <style name="choice_image_high_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/choice_image_item_height</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/color007AFF</item>
        <item name="android:textSize">16dp</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="tag_cloud_item">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">11dp</item>
        <item name="android:paddingRight">11dp</item>
        <item name="android:paddingTop">2.4dp</item>
        <item name="android:paddingBottom">2.4dp</item>
        <item name="android:textColor">@color/tag_bg_color_selector</item>
        <item name="android:textSize">14dp</item>
        <item name="android:background">@drawable/tag_cloud_item_selector</item>
    </style>

    <style name="user_level">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:minWidth">27dp</item>
        <item name="android:layout_height">@dimen/space_14</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">9dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="ListViewNormal">
        <item name="android:scrollbars">none</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:listSelector">@color/transparent</item>
        <item name="android:cacheColorHint">@color/transparent</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
    </style>

    <style name="ActivityBackground">
        <item name="android:background">@color/colorF5F5F5</item>
    </style>

    <!-- 自定义对话框 -->
    <style name="mydialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 背景模糊 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- 自定义loading dialog -->
    <style name="loading_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item> <!-- 边框 -->
        <item name="android:windowIsFloating">true</item> <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item> <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/transparent</item><!-- @android:color/transparent -->
        <item name="android:windowBackground">@android:color/transparent</item><!-- @null -->
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:backgroundDimEnabled">false</item> <!-- 模糊 -->
    </style>

    <style name="mydialogAnimation" parent="android:Animation">
        <item name="@android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <!-- //进入时的动画 -->
        <item name="@android:windowExitAnimation">@anim/dialog_exit_anim</item>
        <!-- //退出时的动画 -->
    </style>

    <style name="MyReplayLike">
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">@color/color403B37</item>
        <item name="android:drawablePadding">6dp</item>
    </style>

    <style name="my_coins_pay_way">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:clickable">false</item>
        <item name="android:textColor">@color/pay_button_color_selector</item>
        <item name="android:textSize">13dp</item>
        <item name="android:button">@null</item>
    </style>

    <style name="my_coins_pay_way_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/space_1</item>
        <item name="android:layout_weight">1</item>
        <item name="android:clickable">false</item>
        <item name="android:background">@color/colorFFC50E</item>
    </style>


    <style name="my_coins_label">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/color666666</item>
    </style>


    <!-- 私信开始 -->
    <style name="chat_item_date">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@drawable/round_10px_black10</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10dp</item>
    </style>

    <style name="chat_item_avatar">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:focusable">false</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="placeholderImage">@drawable/blank_icon_avatar</item>
        <item name="roundingBorderColor">@color/black10</item>
        <item name="roundingBorderWidth">1px</item>
    </style>

    <style name="chat_item_content">
        <item name="android:clickable">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">0dp</item>
    </style>

    <style name="chat_item_text_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">false</item>
        <item name="android:focusable">false</item>
        <item name="android:textColor">@color/color2E2E2E</item>
        <item name="android:textSize">16dp</item>
    </style>

    <style name="chat_item_image_content">
        <item name="android:clickable">true</item>
        <item name="android:focusable">false</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">150dp</item>
        <item name="android:maxHeight">150dp</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:minWidth">50dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <!-- 私信结束 -->

    <style name="newest_item_horizontal_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/newest_item_divider_width</item>
