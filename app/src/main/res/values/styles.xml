<resources>

    <style name="AppBase" parent="Theme.AppCompat.Light.NoActionBar" />

    <!-- Base com.tlv.live.application theme. -->
    <style name="AppTheme" parent="AppBase">
        <!-- Customize your theme here.
        -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppThemeLive" parent="AppBase">
        <!-- Customize your theme here.

        <item name="android:windowBackground">@android:color/transparent</item>
        -->
        <item name="android:windowIsTranslucent">false</item>
    </style>

    <style name="ActivityTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <!--<item name="android:windowContentOverlay">@null</item>-->
        <!--<item name="android:windowAnimationStyle">@style/animation_translucent_translate</item>-->
        <item name="android:windowBackground">@color/color262630</item>
    </style>

    <!-- window 透明的-->
    <style name="Theme.AppCompat.Translucent" parent="AppTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="NoTranslucentActivityTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>

    <style name="TranslucentActivityTheme" parent="AppTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/animation_translucent_translate</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:statusBarColor">@color/transparent</item>
    </style>

    <style name="animation_translucent_translate" parent="@android:style/Animation.Translucent">
        <item name="android:activityOpenEnterAnimation">@anim/slide_left_in</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_right_in</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_right_out</item>
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
    </style>

    <style name="LoginActivity" parent="AppTheme">
        <item name="android:windowBackground">@drawable/launcher_bg_xml</item>
    </style>


    <style name="Transparent" parent="AppTheme">
        <item name="android:windowBackground">@color/color33000000</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="horizontal_light_thin_divider">
        <item name="android:layout_height">2px</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:background">@color/color_gray_d9d9d9</item>
    </style>

    <style name="SwipeBackLayout">
        <item name="edge_size">@dimen/px100</item>
        <item name="shadow_left">@mipmap/shadow_left</item>
        <item name="shadow_right">@mipmap/shadow_right</item>
        <item name="shadow_bottom">@mipmap/shadow_bottom</item>
    </style>


    <style name="back_head_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/head_height</item>
        <!--<item name="android:background">@color/colorFAFAFA</item>-->
        <item name="android:background">@color/white</item>
        <item name="android:alpha">1</item>
    </style>

    <style name="back_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:paddingLeft">9.3dp</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/head_text_selector</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="title_back_view" parent="back_text">
        <item name="android:textColor">@color/color333333</item>
        <item name="android:paddingLeft">11dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:drawablePadding">-4dp</item>
        <item name="android:drawableLeft">@drawable/title_icon_back_selector</item>
        <item name="textSize">@dimen/space_14</item>
    </style>

    <style name="userinfo_back_view" parent="title_back_view">
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingRight">@dimen/space_5</item>
        <item name="android:drawableLeft">@drawable/title_icon_back02_selector</item>
    </style>

    <style name="back_head_title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17dp</item>
    </style>

    <style name="my_item_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/my_info_item_height</item>
        <item name="android:background">@drawable/list_item_selector</item>
    </style>

    <style name="my_activity_item_style" parent="my_item_style">
        <item name="android:paddingLeft">2dp</item>
    </style>

    <style name="divider">
        <item name="android:background">@color/default_divider</item>
    </style>

    <style name="vertical_divider" parent="divider">
        <item name="android:layout_width">1px</item>
    </style>

    <style name="horizontal_divider" parent="divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

    <style name="horizontal_divider_colorE9E9E9">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/colorE9E9E9</item>
    </style>

    <style name="head_divider" parent="divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/colorC8C8C8</item>
    </style>

    <style name="horizontal_divider_margin_left_15dp" parent="horizontal_divider">
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <style name="horizontal_divider_margin_left_50dp" parent="horizontal_divider">
        <item name="android:layout_marginLeft">50dp</item>
    </style>

    <style name="my_vertical_divider" parent="vertical_divider">
        <item name="android:layout_height">@dimen/space_24</item>
        <item name="android:background">@color/white30</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>


    <style name="my_item_left_text">
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/color333333</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/space_15</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="my_item_right_text">
        <item name="android:textColor">@color/color999999</item>
        <item name="android:textSize">15dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="my_count_info">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="contribution_list_face">
        <item name="android:layout_width">@dimen/contribution_list_face_width</item>
        <item name="android:layout_height">@dimen/contribution_list_face_width</item>
