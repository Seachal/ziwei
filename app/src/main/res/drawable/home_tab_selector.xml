<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="false">
        <shape>
            <stroke android:width="@dimen/px1" android:color="@color/color333333" />
            <solid android:color="@color/white" />

        </shape>
    </item>

    <item android:state_selected="true">
        <shape>
            <stroke android:width="@dimen/px1"
                android:color="@color/color333333" />
            <solid android:color="@color/color333333" />
        </shape>
    </item>


</selector>