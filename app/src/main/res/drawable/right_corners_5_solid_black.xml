<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="false">
        <shape>
            <stroke android:width="@dimen/px1" android:color="@color/color333333" />
            <solid android:color="@color/white" />
            <corners
                android:topRightRadius="@dimen/space_5"
                android:bottomRightRadius="@dimen/space_5"/>
        </shape>
    </item>


    <item android:state_selected="true">
        <shape>
            <stroke android:width="@dimen/px1"
                android:color="@color/color333333" />
            <solid android:color="@color/color333333" />
            <corners
                android:topRightRadius="@dimen/space_5"
                android:bottomRightRadius="@dimen/space_5"/>
        </shape>
    </item>


</selector>